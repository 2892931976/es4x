{
  "name": "es-generator",
  "generators": [
    {
      "kind": "class",
      "incremental": true,
      "filename": "if (concrete) { 'npm/index.js' } else null",
      "templateFileName": "module.js.templ"
    },
    {
      "kind": "dataObject",
      "incremental": true,
      "filename": "if (concrete) { 'npm/options.js' } else null",
      "templateFileName": "options.js.templ"
    },
    {
      "kind": "enum",
      "incremental": true,
      "filename": "'npm/enums.js'",
      "templateFileName": "enums.js.templ"
    },
    {
      "kind": "module",
      "filename": "'npm/package.json'",
      "templateFileName": "package.json.templ"
    },
    {
      "kind": "module",
      "filename": "'npm/README.md'",
      "templateFileName": "README.md.templ"
    },

    {
      "kind": "class",
      "incremental": true,
      "filename": "'npm/index.d.ts'",
      "templateFileName": "module.d.ts.templ"
    },
    {
      "kind": "dataObject",
      "incremental": true,
      "filename": "'npm/options.d.ts'",
      "templateFileName": "options.d.ts.templ"
    },
    {
      "kind": "enum",
      "incremental": true,
      "filename": "'npm/enums.d.ts'",
      "templateFileName": "enums.d.ts.templ"
    }
  ]
}
